	{
		if(set_button(mode))
		{
			return;
		}
		
		
		
		
	}
	
	public boolean set_lenth(int _width)
	{
		
		
		return true;
	}

	
	
	
	public boolean set_button(int count)
	{
		if(count >= 3 || count <= 0)
		{
			return false;
		}
		button = count;
		
		return true;
	}
}
